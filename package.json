{
  "name": "netsbloxMobile",
  "version": "0.0.1",
  "config": {},
  "author": "Hamid Zare",
  "homepage": "https://netsblox.org",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build; cd netsblox && npm run postinstall && cd .. && node ./bin/build-nb-client-js.js && node ./bin/build-nb-client-html.js;",
    "build-watch-nb": "nodemon -e js --watch netsblox/src/browser --watch src --watch bin --exec 'node ./bin/build-nb-client-js.js'",
    "build-stable": "rm -r netsblox/src/browser; ln -s $PWD/netsblox/src/client $PWD/netsblox/src/browser; ionic-app-scripts build; cd netsblox && npm run postinstall && cd .. && node ./bin/build-nb-client-js.compat.js && node ./bin/build-nb-client-html.compat.js;",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve"
  },
  "dependencies": {
    "@angular/animations": "12.2.5",
    "@angular/common": "12.2.5",
    "@angular/compiler": "12.2.5",
    "@angular/compiler-cli": "12.2.5",
    "@angular/core": "12.2.5",
    "@angular/forms": "12.2.5",
    "@angular/platform-browser": "12.2.5",
    "@angular/platform-browser-dynamic": "12.2.5",
    "@ionic-native/app-preferences": "^5.36.0",
    "@ionic-native/core": "^5.36.0",
    "@ionic-native/device": "^5.36.0",
    "@ionic-native/diagnostic": "^5.36.0",
    "@ionic-native/dialogs": "^5.36.0",
    "@ionic-native/geolocation": "^5.36.0",
    "@ionic-native/http": "^5.36.0",
    "@ionic-native/in-app-browser": "^5.36.0",
    "@ionic-native/keyboard": "^5.36.0",
    "@ionic-native/location-accuracy": "^5.36.0",
    "@ionic-native/screen-orientation": "^5.36.0",
    "@ionic-native/splash-screen": "^5.36.0",
    "@ionic-native/status-bar": "^5.36.0",
    "@ionic/core": "^5.7.0",
    "@ionic/storage": "^3.0.6",
    "axios": "^0.18.0",
    "commander": "^2.16.0",
    "cordova-android": "7.1.4",
    "cordova-browser": "^6.0.0",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-advanced-http": "^3.2.1",
    "cordova-plugin-app-preferences": "^0.99.3",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-dialogs": "^2.0.2",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-geolocation": "^4.1.0",
    "cordova-plugin-inappbrowser": "^5.0.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-request-location-accuracy": "^2.3.0",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.5",
    "cordova.plugins.diagnostic": "^6.0.3",
    "dot": "^1.1.2",
    "es6-promise-plugin": "^4.2.2",
    "ionic-angular": "^3.9.10",
    "ionicons": "3.0.0",
    "js-sha512": "^0.6.0",
    "rxjs": "6.5.3",
    "sw-toolbox": "^3.6.0",
