{
  "name": "netsbloxMobile",
  "version": "0.0.1",
  "config": {},
  "author": "Hamid Zare",
  "homepage": "https://netsblox.org",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build; cd netsblox && npm run postinstall && cd .. && node ./bin/build-nb-client-js.js && node ./bin/build-nb-client-html.js;",
    "build-watch-nb": "nodemon -e js --watch netsblox/src/browser --watch src --watch bin --exec 'node ./bin/build-nb-client-js.js'",
    "build-stable": "rm -r netsblox/src/browser; ln -s $PWD/netsblox/src/client $PWD/netsblox/src/browser; ionic-app-scripts build; cd netsblox && npm run postinstall && cd .. && node ./bin/build-nb-client-js.compat.js && node ./bin/build-nb-client-html.compat.js;",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve"
  },
  "dependencies": {
    "@angular/common": "4.4.4",
    "@angular/compiler": "4.4.4",
    "@angular/compiler-cli": "4.4.4",
    "@angular/core": "4.4.4",
    "@angular/forms": "4.4.4",
    "@angular/http": "4.4.4",
    "@angular/platform-browser": "4.4.4",
    "@angular/platform-browser-dynamic": "4.4.4",
    "@ionic-native/app-preferences": "^4.7.0",
    "@ionic-native/core": "4.3.2",
    "@ionic-native/device": "^4.5.3",
    "@ionic-native/diagnostic": "^4.5.3",
    "@ionic-native/geolocation": "^4.5.2",
    "@ionic-native/http": "^4.4.2",
    "@ionic-native/in-app-browser": "^4.3.3",
    "@ionic-native/keyboard": "^4.5.3",
    "@ionic-native/location-accuracy": "^4.5.2",
    "@ionic-native/screen-orientation": "^4.4.2",
    "@ionic-native/splash-screen": "4.3.2",
    "@ionic-native/status-bar": "^4.3.2",
    "@ionic/storage": "2.0.1",
    "commander": "^2.11.0",
    "cordova-android": "6.3.0",
    "cordova-browser": "5.0.3",
    "cordova-ios": "4.5.4",
    "cordova-plugin-advanced-http": "^1.9.0",
    "cordova-plugin-app-preferences": "^0.99.3",
    "cordova-plugin-device": "^1.1.7",
    "cordova-plugin-file": "^6.0.0",
    "cordova-plugin-geolocation": "^4.0.1",
    "cordova-plugin-inappbrowser": "^1.7.2",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-request-location-accuracy": "^2.2.2",
    "cordova-plugin-screen-orientation": "^2.0.2",
    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-statusbar": "^2.4.0",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova.plugins.diagnostic": "^3.9.1",
    "dot": "^1.1.2",
    "es6-promise-plugin": "^4.1.1",
    "ionic-angular": "3.8.0",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "jquery": "^3.2.1",
    "js-sha512": "^0.6.0",
    "q": "^1.5.1",
    "rxjs": "5.4.3",
    "sw-toolbox": "3.6.0",
    "time-ago": "^0.1.0",
    "uglify-js": "^3.2.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.0.1",
    "typescript": "2.3.4"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-inappbrowser": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {
        "GEOLOCATION_USAGE_DESCRIPTION": "To locate you"
      },
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-screen-orientation": {},
      "cordova.plugins.diagnostic": {},
      "cordova-plugin-request-location-accuracy": {},
      "cordova-plugin-app-preferences": {}
    },
    "platforms": [
      "ios",
      "android",
      "browser"
    ]
  }
}