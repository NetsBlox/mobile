{
  "name": "netsbloxMobile",
  "version": "0.0.1",
  "config": {},
  "author": "Hamid Zare",
  "homepage": "https://netsblox.org",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build; cd netsblox && npm run postinstall && cd .. && node ./bin/build-nb-client-js.js && node ./bin/build-nb-client-html.js;",
    "build-watch-nb": "nodemon -e js --watch netsblox/src/browser --watch src --watch bin --exec 'node ./bin/build-nb-client-js.js'",
    "build-stable": "rm -r netsblox/src/browser; ln -s $PWD/netsblox/src/client $PWD/netsblox/src/browser; ionic-app-scripts build; cd netsblox && npm run postinstall && cd .. && node ./bin/build-nb-client-js.compat.js && node ./bin/build-nb-client-html.compat.js;",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve"
  },
  "dependencies": {
    "@angular/animations": "5.2.11",
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/forms": "5.2.11",
    "@angular/http": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@ionic-native/app-preferences": "^4.11.0",
    "@ionic-native/core": "~4.11.0",
    "@ionic-native/device": "^4.11.0",
    "@ionic-native/diagnostic": "^4.11.0",
    "@ionic-native/dialogs": "^4.11.0",
    "@ionic-native/geolocation": "^4.11.0",
    "@ionic-native/http": "^4.11.0",
    "@ionic-native/in-app-browser": "^4.11.0",
    "@ionic-native/keyboard": "^4.11.0",
    "@ionic-native/location-accuracy": "^4.11.0",
    "@ionic-native/screen-orientation": "^4.11.0",
    "@ionic-native/splash-screen": "~4.11.0",
    "@ionic-native/status-bar": "~4.11.0",
    "@ionic/storage": "2.1.3",
    "axios": "^0.18.1",
    "commander": "^2.16.0",
    "cordova-android": "7.1.4",
    "cordova-browser": "5.0.3",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-advanced-http": "^3.1.0",
    "cordova-plugin-app-preferences": "git+https://bitbucket.org/packt-internal/cordova-plugin-app-preferences.git",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-dialogs": "^2.0.2",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-geolocation": "^4.0.2",
    "cordova-plugin-inappbrowser": "^4.0.0",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-request-location-accuracy": "^2.3.0",
    "cordova-plugin-screen-orientation": "^2.0.2",
    "cordova-plugin-splashscreen": "^4.1.0",
    "cordova-plugin-statusbar": "^2.4.3",
    "cordova-plugin-whitelist": "^1.3.4",
    "cordova.plugins.diagnostic": "^3.9.2",
    "dot": "^1.1.2",
    "es6-promise-plugin": "^4.2.2",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "js-sha512": "^0.6.0",
    "q": "^1.5.1",
    "rxjs": "5.5.11",
    "sw-toolbox": "3.6.0",
    "time-ago": "^0.1.0",
    "uglify-js": "^3.11.6",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.11",
    "typescript": "~2.6.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-inappbrowser": {},
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {
        "GEOLOCATION_USAGE_DESCRIPTION": "To locate you"
      },
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-advanced-http": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-screen-orientation": {},
      "cordova.plugins.diagnostic": {},
      "cordova-plugin-request-location-accuracy": {},
      "cordova-plugin-app-preferences": {},
      "cordova-plugin-geolocation": {
        "GEOLOCATION_USAGE_DESCRIPTION": "To provide location services."
      },
      "cordova-plugin-dialogs": {}
    },
    "platforms": [
      "browser",
      "android",
      "ios"
    ]
  }
}